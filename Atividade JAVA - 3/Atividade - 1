package familia44_3;

import java.util.Scanner;

public class Exercicio1 {

	public static void main(String[] args) {

		Scanner leia = new Scanner(System.in);
		int [] A = new int [6];
		int var;
		
		A[0] = 1;
		A[1] = 0;
		A[2] = 5;
		A[3] = -2;
		A[4] = -5;
		A[5] = 7;

		var = A[0] + A[1] + A[5];
		System.out.println(var);
		A[4] = 100;
		
		for(int i=0;i<6;i++) {
			System.out.println(A[i]);
		}
	}

}
